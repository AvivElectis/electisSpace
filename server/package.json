{
    "name": "electisspace-server",
    "version": "1.0.0",
    "description": "electisSpace Backend Server - Node.js API with PostgreSQL",
    "main": "dist/server.js",
    "scripts": {
        "dev": "tsx watch src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "db:generate": "prisma generate",
        "db:push": "prisma db push",
        "db:migrate": "prisma migrate dev",
        "db:studio": "prisma studio",
        "db:seed": "tsx prisma/seed.ts",
        "test": "vitest",
        "test:coverage": "vitest --coverage",
        "lint": "eslint src --ext .ts",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [
        "electisspace",
        "esl",
        "solum",
        "api",
        "express"
    ],
    "author": "Aviv Ben Waiss <aviv@electis.co.il>",
    "license": "PROPRIETARY",
    "dependencies": {
        "@prisma/client": "^6.0.0",
        "bcrypt": "^5.1.1",
        "bullmq": "^5.0.0",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.4.0",
        "express": "^4.21.0",
        "express-rate-limit": "^7.5.0",
        "helmet": "^8.0.0",
        "ioredis": "^5.4.0",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.0",
        "socket.io": "^4.8.0",
        "uuid": "^11.0.0",
        "zod": "^3.24.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/compression": "^1.7.5",
        "@types/cookie-parser": "^1.4.7",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/morgan": "^1.9.9",
        "@types/node": "^22.0.0",
        "@types/uuid": "^10.0.0",
        "eslint": "^9.0.0",
        "prisma": "^6.0.0",
        "tsx": "^4.19.0",
        "typescript": "^5.7.0",
        "vitest": "^2.0.0"
    },
    "engines": {
        "node": ">=20.0.0"
    }
}